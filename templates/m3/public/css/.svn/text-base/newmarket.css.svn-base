/*我的菜单*/

.myCart{ width:219px;position:absolute; z-index:999; right:0px; top:-4px; }
.myCart .icon_cart{ display:block; position:absolute; left:-55px; top:0px;width:55px; height:44px;background:#f60; color:#fff; font-size:18px; padding-left:45px; line-height:44px; z-index:1; }
