 <{extends file="<{$tempdir}>/public/admin.html"}>

 <{block name=controlname}>管理员列表<{/block}>
 <{block name=bodylist}>
  <div style="width:auto;overflow-x:hidden;overflow-y:auto">
           <div class="tags">

      	 	<div id="tagstitle"></div>

          <div id="tagscontent">

            <div id="con_one_1">

              <div class="table_td" style="margin-top:10px;">

              <form method="post" action="" onsubmit="return remind();">

                  <table border="0" cellspacing="0" cellpadding="0" class="list" name="table" id="table" width="100%">

                    <thead>

                      <tr>

                        <th width="60px"><input type="checkbox" id="chkall" name="chkall" onclick="checkall(this.form, 'id')"></th>

                        <th align="center">用户名称</th>

                        <th align="center">登陆时间</th>

                        <th align="center">创建时间</th>
                        <th align="center">登陆IP</th>

                        <th align="center">操作</th>

                      </tr>

                    </thead>

                     <tbody>
                      <{load_data assign="list" table="admin" showpage="true"  }>
                      <{foreach from=$list['list'] item=items}>
                      <tr class="s_out" onmouseover="this.bgColor='#F5F5F5';" onmouseout="this.bgColor='ffffff';" bgcolor="#ffffff">
                        <td align="center" ><input type="checkbox" name="id[]" value="<{$items['uid']}>"></td>
                        <td align="center"><{$items['username']}></td>
                        <td align="center"> <{$items['logintime']|date_format:"%Y-%m-%d %H:%M:%S"}> </td>
                        <td align="center"><{$items['time']|date_format:"%Y-%m-%d %H:%M:%S"}></td>
                        <td align="center"><{$items['loginip']}></td>
                        <td align="center">
                        	<a href="<{ofunc type=url link="/adminpage/member/module/addadmin/id/<{$items['uid']}>"}>"><img src="<{$siteurl}>/templates/<{$tempdir}>/public/images/admin/edit.jpg"></a>
                        	<a onclick="return remind(this);" href="<{ofunc type=url link="/adminpage/member/module/deladmin/datatype/json/id/<{$items['uid']}>"}>"><img src="<{$siteurl}>/templates/<{$tempdir}>/public/images/admin/del.jpg"></a>

                        	</td>
                      </tr>
                       <{/foreach}>

                    </tbody>

                  </table>

                <div class="blank20"></div>



                </form>

                <div class="page_newc">
                 	      <div class="select_page">

                 	     </div>
                       <div class="show_page"><ul><{$list['pagecontent']}></ul></div>
                 </div>

                <div class="blank20"></div>

              </div>

            </div>

          </div>

        </div>


  </div>

  </div>
<{/block}>
